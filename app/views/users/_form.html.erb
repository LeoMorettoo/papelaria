<%= form_for(@user, html: {class:'form-horizontal'}) do |f| %>
  <% if @user.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@user.errors.count, "error") %> prohibited this user from being saved:</h2>

      <ul>
      <% @user.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
 <div class="field form-group">
    <%= f.label :name, class: 'col-sm-2 control-label' %>
     <div class="col-sm-10">
      <%= f.text_field :name, class: 'form-control', id: 'name', name: 'name', placeholder: 'xablau nascimento' %>
    </div>
  </div>
  <div class="field form-group">
    <%= f.label :email, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
      <%= f.text_field :email, class: 'form-control', id: 'email', name: 'email', placeholder: 'xablaunascimento@gmail.com' %>
    </div>
  </div>
  <div class="field form-group">
    <%= f.label :login, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
      <%= f.text_field :login, class: 'form-control', id: 'login', name: 'login' %>
    </div>
  </div>
  <div class="field form-group">
    <%= f.label :password, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
      <%= f.text_field :password, class: 'form-control', id: 'password', name: 'password',type: 'password' %>
    </div>
  </div>
  <div class="field form-group">
    <%= f.label :cpf, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
      <%= f.text_field :cpf, class: 'form-control', id: 'cpf', name: 'cpf', placeholder: 'xxx.xxx.xxx-xx' %>
    </div>
  </div>
  <div class="field form-group">
    <%= f.label :rg, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
      <%= f.text_field :rg, class: 'form-control', id: 'rg', name: 'rg', placeholder: 'xx.xxx.xxx-x' %>
    </div>
  </div>
  <div class="field form-group">
    <%= f.label :createDate, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
      <%= f.datetime_select :createDate, class: 'form-control', id: 'createDate', name: 'createDate' %>
    </div>
  </div>
  <div class="field form-group">
    <%= f.label :updateDate, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
      <%= f.datetime_select :updateDate, class: 'form-control', id: 'updateDate', name: 'updateDate' %>
    </div>
  </div>
  <div class="field form-group">
    <%= f.label :comments, class: 'col-sm-2 control-label' %>
    <div class="col-sm-10">
      <%= f.text_area :comments, class: 'form-control', id: 'comments', name: 'comments' %>
    </div>
  </div>
  <div class="actions form-group">
    <div class="col-sm-offset-2 col-sm-10">
    <%= f.submit class: 'btn btn-primary',value: 'Send'%>
      
    </div>
  </div>
<% end %>
